<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_set_up"
    app:startDestination="@id/NameSelectionFragment">

    <fragment
        android:id="@+id/NameSelectionFragment"
        android:name="dev.sotoestevez.allforone.ui.blank.NameSelectionFragment"
        android:label="@string/title_fragment_name_selection"
        tools:layout="@layout/fragment_name_selection">

        <action
            android:id="@+id/action_NameSelection_to_RoleSelection"
            app:destination="@id/RoleSelectionFragment" />
    </fragment>
    <fragment
        android:id="@+id/RoleSelectionFragment"
        android:name="dev.sotoestevez.allforone.ui.blank.RoleSelectionFragment"
        android:label="@string/title_fragment_role_selection"
        tools:layout="@layout/fragment_role_selection">

        <action
            android:id="@+id/action_RoleSelection_to_NameSelection"
            app:destination="@id/NameSelectionFragment" />
    </fragment>
</navigation>